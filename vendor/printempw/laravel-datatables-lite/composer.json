{
    "name": "printempw/laravel-datatables-lite",
    "description": "A lite version of yajra/laravel-datatables, with HTML builders & exporting removed.",
    "keywords" : ["laravel4","laravel5","laravel","datatables","datatable","datatables jquery plugin"],
    "license": "MIT",
    "authors": [
        {
            "name": "Arjay Angeles",
            "email": "aqangeles@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "~5.0",
        "illuminate/database": "~5.0",
        "illuminate/view": "~5.0",
        "illuminate/http": "~5.0",
        "illuminate/filesystem": "~5.0",
        "league/fractal": "~0.14"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~4.0"
    },
    "suggest": {
        "barryvdh/laravel-snappy": "Allows exporting of dataTable to PDF using the print view."
    },
    "autoload": {
        "psr-4": {
            "Yajra\\Datatables\\": "src/"
        }
    },
    "minimum-stability": "stable"
}
